{
  "name": "tiddlywiki",
  "preferGlobal": "true",
  "version": "5.1.3-prerelease",
  "author": "Jeremy Ruston <jeremy@jermolene.com>",
  "description": "a non-linear personal web notebook",
  "contributors": [
    {
      "name": "Jeremy Ruston",
      "email": "jeremy@jermolene.com"
    }
  ],
  "bin": {
    "tiddlywiki": "./tiddlywiki.js"
  },
  "main": "./boot/boot.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jermolene/TiddlyWiki5.git"
  },
  "keywords": [
    "tiddlywiki",
    "tiddlywiki5",
    "wiki"
  ],
  "dependencies": {},
  "devDependencies": {},
  "bundleDependencies": [],
  "license": "BSD",
  "engines": {
    "node": ">=0.8.2"
  },
  "scripts": {
    "start": "node tiddlywiki.js ./editions/tw5.com-server --server 8080 ",
    "serve": "node tiddlywiki.js --npmserve",
    "build": "node tiddlywiki.js --npmbuild",
    "test":  "node tiddlywiki.js ./editions/test --build index && echo !!!!!!!!!!! In the browser, open: ./editions/test/output/test.html",
    "//":"-- TODO check lazy --",
    "lazy":  "node tiddlywiki.js ./editions/tw5.com-server --server 8080 $:/core/save/lazy-images"
  },
  "config" : {
    "//":"--- default serve configuraton ---",
    "serve_edition": "./editions/tw5.com-server",
    "serve_port" : "8080",
    "serve_host": "",
    "//":"--- default build configuration ---",
    "build_edition": "./editions/tw5.com",
    "build_command": "--build",
    "build_element": "",
    "//":"--- more info for debugging ---",
    "verbose": "--verbose"
  }
}
